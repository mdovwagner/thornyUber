{
  "name": "thornyubers",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@babel/cli": "^7.13.14",
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "boardgame.io": "^0.42.0",
    "dev-server": "^0.1.0",
    "esm": "^3.2.25",
    "koa-static": "^5.0.0",
    "nodemon": "^2.0.6",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-digraph": "^7.0.0-alpha.5",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.0",
    "use-sound": "^2.0.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "SINGLE_PORT=1 node -r esm server.js",
    "dev": "REACT_APP_NO_LOBBY=1 react-scripts start",
    "dev-lobby": "REACT_APP_ENV=dev react-scripts start",
    "dev-server": "ENV=dev nodemon --inspect --trace-warnings -r esm server.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "node -r esm server.js",
    "publish:npm": "rm -rf dist && mkdir dist &&  babel src -d dist --copy-files"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "main": "dist/main.js",
  "module": "dist/main.js",
  "babel": {
    "presets": [
      "@babel/react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
